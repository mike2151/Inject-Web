{
    "name": "Inject Web",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Inject HTML snippets into your desired location on your current web page",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "editor.html"
    },
    "permissions": ["activeTab", "tabs","<all_urls>"],
    "background": {
    "scripts": ["editor.js"],
    "persistent": false
    },
    "content_scripts": [
        
        {
        "js": [ "jquery.min.js"],
        "matches": [ "http://*/*", "https://*/*"]
      }
        
    ]
}
